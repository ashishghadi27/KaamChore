<!-- <?xml version="1.0" encoding="utf-8"?> -->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="550dip" />
            <stroke android:width="5dip" android:color="#ff6d00" />
            <gradient android:angle="0" android:startColor="#ff6d00" android:endColor="#ff6d00"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="550dip" />
            <stroke android:width="5dip" android:color="#ff6d00" />
            <solid android:color="#ff6d00"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="550dip" />
            <stroke android:width="1dip" android:color="#ff6d00" />
            <gradient android:angle="0" android:startColor="#ff6d00" android:endColor="#ff6d00" />
        </shape>
    </item>
</selector>